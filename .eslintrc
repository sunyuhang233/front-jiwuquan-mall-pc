{
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["@unocss", "plugin:vue/vue3-essential"],
  "plugins": ["vue"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "parser": "@typescript-eslint/parser"
  },
  "rules": {      
    "semi": ["error", "always"] 
}  
}
